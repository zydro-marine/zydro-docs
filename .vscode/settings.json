{
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "eslint.validate": ["javascript"],
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true
    },
    "files.exclude": {
        "**/node_modules": true,
        "**/npm-debug.log": true,
        "**/.DS_Store": true,
        "**/.idea/": true,
        "**/.idea_modules/": true
    },
    "cmake.configureOnOpen": false,
    "color-picker.languages": ["python", "javascript", "javascriptreact", "typescript"]
}
